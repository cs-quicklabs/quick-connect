  <main class="max-w-7xl pb-10 lg:py-12 lg:px-8 mx-auto">
  <div class="lg:grid lg:grid-cols-12 lg:gap-x-5">
    <%= render "shared/settings_menu" %>
    <main class="max-w-xl pb-12 px-4 lg:col-span-6">
      <%= turbo_frame_tag "contact_form" do %>
        <%= render "user/forms/profile", user: @user %>
      <% end %>
      <!-- This example requires Tailwind CSS v2.0+ -->
      <% if @user.invited_by_id.nil?%>
        <%= render "reset", user: @user %>
      <% end %>
    </main>
  </div>
</main>
