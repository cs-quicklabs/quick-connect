<% @contacts.each do |contact| %>

    <li role="option"  @click="isShowing = false; document.getElementById('search_id').value='<%= contact.id %>';document.getElementById('search').value='<%= contact.decorate.display_name %>'" class="cursor-pointer block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 hover:text-gray-900" id="batch-contact" >
      <%= contact.decorate.display_name %>
    </li>

<% end %>