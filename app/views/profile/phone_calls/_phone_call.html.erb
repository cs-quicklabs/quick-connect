  <li>
<%= turbo_frame_tag dom_id(phone_call) do %>
    <div class="flex mt-6 space-x-3">
      <div class="flex-shrink-0">
        <%= image_tag "male-placeholder-image.png", class: "w-10 h-10 rounded-full" %>
      </div>
      <div>
        <div class="text-sm">
          <a href="#" class="font-medium text-gray-900"><%= phone_call.user.decorate.display_name %></a>
        </div>
        <div class="mt-1 text-sm text-gray-700">
          <p class="whitespace-pre-wrap"><%= phone_call.body.upcase_first %></p>
        </div>
        <div class="mt-2 space-x-2 text-sm">
          <span class="font-medium text-gray-500"><%= display_created_at(phone_call) %></span>
          <% if policy(@contact).edit? %>
            <%= edit_button(edit_profile_phone_call_path(@contact, phone_call)) %>
          <% end %>
          <% if policy(@contact).destroy? %>
            <%= delete_button(profile_phone_call_path(@contact, phone_call)) %>
          <% end %>
        </div>
      </div>
    </div>

<% end %>
  </li>
