<li>
  <div class="relative pb-4">
    <span class="absolute top-4 left-4 -ml-px h-full w-0.5 bg-gray-200" aria-hidden="true"></span>
    <div class="">
    <div class="flex space-x-3">
      <div>
        <span class="flex items-center justify-center w-8 h-8 bg-gray-200 rounded-full ring-2 ring-white">
          <%= image_tag "male-placeholder-image.png", class: "w-7 h-7 rounded-full" %>
        </span>
      </div>
      <div class="min-w-0 flex-1 pt-1.5 flex justify-between space-x-4">
        <div>
          <p class="text-sm text-gray-500">
            <% if event.account.owner_id == event.user_id %>
              You
            <% else %>
              <%= event.user.decorate.display_name %>
            <% end %>
            <%= event.action_context %>
            for this contact.
          </p>
        </div>

        <div class="text-sm text-right text-gray-500 whitespace-nowrap">
          <%= display_created_at(event) %>
        </div>

      </div>



    </div>
                                      <% unless event.trackable.nil? %>

          <div class="p-2 ml-8 text-xs italic font-normal text-gray-500 border border-gray-200 rounded-lg bg-gray-50 dark:bg-gray-600 dark:border-gray-500 dark:text-gray-300 space-x-4">
      <p class="whitespace-pre-wrap"><%= event.trackable.body %></p>
                    <% end %>

    <div>


  </div>
</li>
