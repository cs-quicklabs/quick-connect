<%= turbo_frame_tag dom_id(relative) do %>
  <li class="flex py-4 border-b border-gray-200">
    <div class="flex w-full ml-3">
      <p class="w-full text-sm font-medium text-gray-900"><%= relative.name.titleize %></p>
      <div class="flex justify-end space-x-2">
        <%= edit_button(edit_account_relative_path(relative)) %>
        <%= delete_button(account_relative_path(relative)) %>
      </div>
    </div>
  </li>
<% end %>
