<%= turbo_frame_tag dom_id(contact) do %>
  <div id ="contactes">
    <li class="flex py-3 border-b border-gray-200 cursor-pointer px-2 " data-controller="counter" >
      <div class="flex  w-full  " >
        <a  id="contact_<%= contact.id %>" class="inline-flex w-11/12 hover:underline"  data-reflex="click->contact#show" data-contact-id="<%= contact.id %>">
          <p class=" w-11/12  text-sm font-medium text-gray-900" >
            <%= contact.name.titleize %></p>
        </a>
        <div class="flex justify-end space-x-1">
          <%= edit_button("contacts/#{contact.id}/edit") %>
          <%= delete_button("contacts/#{contact.id}") %>
        </div>
      </div>
    </li>
  </div>
<% end %>
