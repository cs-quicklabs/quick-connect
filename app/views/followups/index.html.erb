<main class="relative z-0 flex-1 overflow-x-auto focus:outline-none">
  <div class="min-h-full">
    <div class="flex flex-wrap items-baseline px-6 py-6 " >
      <h1 class="text-3xl font-bold leading-tight text-gray-900">Follow Ups</h1>
      <p class="mt-1 ml-1 text-sm text-gray-500 truncate">(<%= @firsts.count.to_s %>)</p>
    </div>
    <div class="  ">
      <div class=" grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 md:grid border-t px-1 border-gray-200" style=" grid-template-columns: repeat(4, 358px); width:1440px;">
        <div class=" lg:block grid grid-cols-1 flex  py-2 h-full border-r  ">
          <div class=" relative" style="min-height: 16rem">
            <div class="absolute inset-0  rounded-lg">
              <div class="flex flex-wrap items-baseline px-4 py-2">
                <h3 class="text-xs font-semibold tracking-wider text-gray-500 uppercase" id="teams-headline">Last 30 days</h3>
                <p class="mt-1 ml-1 text-sm text-gray-500 truncate">(<%= @firsts.count.to_s %>)</p>
              </div>
              <nav class="flex-1 overflow-y-scroll   h-[calc(100vh-5.75rem)]  max-content top-16 overscroll-contain" aria-label="Directory" >
                <div class="relative  h-full py-4" >
                  <ul role="list" class="grid grid-cols-1 gap-6  px-4 mr-1" id="firsts">
                    <%= render partial: "card", collection: @firsts, as: :event, cached: false %>
                  </ul>
                </div>
              </nav>
            </div>
          </div>
        </div>
        <div class=" lg:block grid grid-cols-1 flex  py-2 h-full border-r "  >
          <div class=" relative" style="min-height: 16rem">
            <div class="flex flex-wrap items-baseline px-4 py-2 ">
              <h3 class="text-xs font-semibold tracking-wider text-gray-500 uppercase" id="teams-headline">Last 60 days</h3>
              <p class="mt-1 ml-1 text-sm text-gray-500 truncate">(<%= @seconds.count.to_s %>)</p>
            </div>
            <nav class="flex-1 overflow-y-scroll   h-[calc(100vh-6.0rem)] max-content top-16 overscroll-contain h-full  " style=""  aria-label="Directory" >
              <div class="relative  h-full py-4" >
                <ul role="list" class="grid grid-cols-1 gap-6 px-4 mr-1" id="seconds">
                  <%= render partial: "card", collection: @seconds, as: :event, cached: false %>
                </ul>
              </div>
            </nav>
          </div>
        </div>
        <div class=" lg:block grid grid-cols-1 flex  py-2 h-full border-r  "  >
          <div class="h-full relative" style="min-height: 16rem">
            <div class="flex flex-wrap items-baseline px-4 py-2 ">
              <h3 class="text-xs font-semibold tracking-wider text-gray-500 uppercase" id="teams-headline">Last 90 days</h3>
              <p class="mt-1 ml-1 text-sm text-gray-500 truncate">(<%= @thirds.count.to_s %>)</p>
            </div>
            <nav class="flex-1 overflow-y-scroll   h-[calc(100vh-6.0rem)] max-content top-16 overscroll-contain h-full  " style="" aria-label="Directory" >
              <div class="relative  h-full py-4 " >
                <ul role="list" class="grid grid-cols-1 gap-6 px-4 mr-1" id="thirds">
                  <%= render partial: "card", collection: @thirds, as: :event, cached: false %>
                </ul>
              </div>
            </nav>
          </div>
        </div>
        <div class=" lg:block grid grid-cols-1 flex  py-2 h-full border-r "  >
          <div class="h-full relative" style="min-height: 16rem">
            <div class="flex flex-wrap items-baseline px-4 py-2">
              <h3 class="text-xs font-semibold tracking-wider text-gray-500 uppercase" id="teams-headline">More than 100 days</h3>
              <p class="mt-1 ml-1 text-sm text-gray-500 truncate">(<%= @fourths.count.to_s %>)</p>
            </div>
            <nav class="flex-1 overflow-y-scroll  h-[calc(100vh-6.0rem)] max-content top-16 overscroll-contain h-full  " style="" aria-label="Directory" >
              <div class="relative  h-full py-4" >
                <ul role="list" class="grid grid-cols-1 gap-6 px-4 mr-1" id="fourths">
                  <%= render partial: "card", collection: @fourths, as: :event, cached: false %>
                </ul>
              </div>
            </nav>
          </div>
        </div>
      </div>
    </div>
  </div>
</main>
