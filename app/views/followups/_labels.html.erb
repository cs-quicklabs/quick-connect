<% if contact_labels.load.size > 0 %>
  <% contact_labels.uniq.each do |label| %>
    <span class="inline-flex items-center rounded-full px-1.5 py-0.5 text-xs font-medium ring-1 ring-inset ring-<%= label.color %>-500/10 bg-<%= label.color %>-100 text-<%= label.color %>-800 ml-2">
      <%= label.name.upcase_first %>
    </span>
  <% end %>
<% end %>
