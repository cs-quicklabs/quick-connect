<%= turbo_frame_tag dom_id(event) do %>
  <% contact = event.eventable.decorate %>
  <li id= "<%= dom_id(contact) %>" class=" my-3 col-span-1 bg-white rounded-lg  border divide-gray-200   ">
    <div class="pb-1">
      <div class="w-full flex  justify-between  mt-3 pr-2   " style=" margin-bottom:0.5rem; ">
        <div class="flex-1 truncate  relative cursor-pointer flex items-center space-x-3"  style="">
          <div class="flex-shrink-0">
            <%= image_tag "male-placeholder-image.png", style: "margin-left: 0.5rem;", class: "w-10 h-10 rounded-full inline-flex" %>
          </div>
          <a type="button" href="/<%= contact.account.id %>/contacts/<%= contact.id %>/<%= event.trackable.model_name.plural %>" class=" bg-white cursor-pointer hover:underline" target= "_top">
            <div class="flex-1 min-w-0"  style="">
              <p class="text-sm font-medium text-gray-900 truncate ... w-56"><%= contact.display_name %>
              </p>
              <% if !contact.phone.blank? %>
                <svg xmlns="http://www.w3.org/2000/svg" class="inline-flex" style="width: .8rem;height:.8rem" viewBox="0 0 20 20" fill="currentColor">
                  <path d="M2 3a1 1 0 011-1h2.153a1 1 0 01.986.836l.74 4.435a1 1 0 01-.54 1.06l-1.548.773a11.037 11.037 0 006.105 6.105l.774-1.548a1 1 0 011.059-.54l4.435.74a1 1 0 01.836.986V17a1 1 0 01-1 1h-2C7.82 18 2 12.18 2 5V3z" />
                </svg>
                <p class="text-sm text-gray-500 truncate ... inline-flex w-56">    <%= contact.phone %> </p>
              <% else %>
                <svg class="inline-flex" style="width: .8rem;height:.8rem" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                  <path d="M2.003 5.884L10 9.882l7.997-3.998A2 2 0 0016 4H4a2 2 0 00-1.997 1.884z" />
                  <path d="M18 8.118l-8 4-8-4V14a2 2 0 002 2h12a2 2 0 002-2V8.118z" />
                </svg>
                <p class="text-sm text-gray-500 truncate ... inline-flex w-56">  <%= contact.email %>  </p>
              <% end %>
            </div>
          </a>
        </div>
      </div>
      <div class="px-2 flex ">
        <div class="w-0 flex-1 flex" style="flex-direction: column;">
          <p class="text-sm font-normal text-gray-500" >Last Activity: <%= display_date(event.created_at) %> </p>
        </div>
        <% html = "<div class='flex justify-end cursor-pointer' id='untrack' style='flex-direction: column;'    >
                     <svg xmlns='http://www.w3.org/2000/svg' class='h-4 w-4 ml-8 text-gray-500' style='' fill='none' viewBox='0 0 24 24' stroke-width='1.5' stroke='currentColor' class='w-6 h-6'>
               <path stroke-linecap='round' stroke-linejoin='round' d='M15 12H9m12 0a9 9 0 11-18 0 9 9 0 0118 0z' />
             </svg>
                        <p class='text-xs font-normal text-gray-500'>Do not track</p>
                   
                     </div>" %>
        <%= link_to (raw(html)), untrack_contact_path(contact), class: "", data: {
                                                                    controller: "confirmation",
                                                                    "confirmation-message-value": "Are you sure you want to delete this?",
                                                                    action: "confirmation#confirm", turbo_frame: :_top,
                                                                  } %>
      </div>
    </div>
  </li>
<% end %>
