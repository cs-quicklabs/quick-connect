<div class="flex-1 min-w-0" id="title">
  <h3 class="text-2xl font-bold leading-tight text-gray-900 flex-none mt-4">
    <%= highlight_hashtag(journal.title.upcase_first) %>
  </h3>
  <div class="flex mt-4 space-x-3">
    <div>
      <div class="mt-1 space-x-2 text-sm">
        <%if journal.account.owner!=journal.user%>
          <span class="font-medium text-gray-500">Created By <%= journal.user.decorate.display_name %> on <%= display_created_at(journal) %></span>
        <% end %>
      </div>
    </div>
  </div>
</div>
