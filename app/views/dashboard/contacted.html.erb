<turbo-frame id="contacted">
  <ul role="list" class="" style="grid-template-columns: repeat(10, 40px); gap: 1rem;  display: grid;" id="side-menu">
    <% @contacted.each do |recent| %>
      <li>
        <a  href="/<%= recent.account.id %>/contacts/<%= recent.eventable.id %>/<%= recent.trackable.class.model_name.plural %>" class="  ">
          <span role="img" class="inline-flex h-10 w-10 items-center justify-center rounded-full bg-gray-500"  title="<%= recent.eventable.decorate.display_name %>" >
            <span class="font-medium leading-none text-white"  > <%= display_image(recent.eventable) %>
            </span>
          </span>
        </a>
      </li>
    <% end %>
  </ul>
</turbo-frame>
